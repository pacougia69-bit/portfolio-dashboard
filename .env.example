# Backend OAuth Server URL (used by server-side code)
# In production, set this to your OAuth server URL
# In development, use http://localhost:3000
OAUTH_SERVER_URL=

# Frontend OAuth Portal URL (used by client-side code)
# In production, set this to your OAuth portal URL
# In development, use http://localhost:3000
VITE_OAUTH_PORTAL_URL=

# Application URL (used for redirects and base URL)
# In production, set this to your Railway app URL (e.g., https://your-app.up.railway.app)
# In development, use http://localhost:3000
VITE_APP_URL=

VITE_APP_ID=demo-app

VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=
